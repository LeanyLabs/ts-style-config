{
  "extends": "./tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "module": "commonjs",
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "strictPropertyInitialization": false,
    "types": ["node", "jest"]
  },
  "include": ["src/**/*.ts", "test/**/*.ts"],
  "exclude": ["test/fixtures/**/*.*", "template/**/*.*"]
}
